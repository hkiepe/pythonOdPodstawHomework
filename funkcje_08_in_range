def in_range(x, min, max):
    if x >= min and x <= max:
        return True
    else:
        return False


x = 10

min = float(input("Please type a minimum number: "))
max = float(input("Please type a maximum number: "))

if in_range(float(x), min, max):
    print(str(x) + " is in range")
else:
    print(str(x) + " is not in range")
